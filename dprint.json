	"useTabs": true,
	"indentWidth": 4,
	"lineWidth": 120,
	"newLineKind": "lf",
	"json": {
		"trailingCommas": "never"
	},
	"markdown": {},
	"oxc": {},
	"yaml": {},
	"excludes": [
		"**/*-lock.json",
		"**/node_modules",
		"build",
		"test_data",
		"test_data_cli"
	],
	"plugins": [
		"https://plugins.dprint.dev/json-0.21.1.wasm",
		"https://plugins.dprint.dev/markdown-0.20.0.wasm",
		"https://plugins.dprint.dev/oxc-0.3.0.wasm",
		"https://plugins.dprint.dev/g-plane/pretty_yaml-v0.6.0.wasm"
	]
}
